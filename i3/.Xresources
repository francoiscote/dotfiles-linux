! XFT
Xft.antialias:  1
Xft.hinting:    1
Xft.autohint:   0
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb
!Xft.dpi:        96

! Xcursor
Xcursor.theme: OpenZone_Black_Slim
Xcursor.size:  16

! Xterm
XTerm*faceName: Operator Mono SSm Medium:pixelsize=16:antialias=true
XTerm*selectToClipboard: true
